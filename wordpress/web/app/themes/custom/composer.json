{
    "name": "rareloop/lumberjack",
    "license": "MIT",
    "type": "wordpress-theme",
    "description": "Lumberjack WordPress starter theme",
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "authors": [
        {
            "name": "Adam Tomat",
            "email": "adam.tomat@rareloop.com",
            "homepage": "http://rareloop.com"
        },
        {
            "name": "Joe Lambert",
            "email": "joe@rareloop.com",
            "homepage": "http://rareloop.com"
        }
    ],
    "keywords": [
        "lumberjack", "composer", "wordpress", "wp", "theme", "timber", "twig", "mvc", "dry"
    ],
    "require": {
        "composer/installers": "^1.0",
        "rareloop/lumberjack-core": "^5.0",
        "stoutlogic/acf-builder": "^1.10",
        "phpunit/phpunit": "^9.0",
        "tightenco/collect": "5.8.38"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require-dev": {
        "hellonico/timber-dump-extension": "^1.0",
        "djboris88/timber-commented-include": "^1.1"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
